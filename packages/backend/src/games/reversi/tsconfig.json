{
	"compilerOptions": {
		"noEmitOnError": false,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"experimentalDecorators": true,
		"declaration": true,
		"sourceMap": false,
		"target": "es2017",
		"module": "commonjs",
		"removeComments": false,
		"noLib": false,
		"outDir": "./built",
		"rootDir": "./"
	},
	"compileOnSave": false,
	"include": [
		"./core.ts"
	]
}
